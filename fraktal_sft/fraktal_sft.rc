        END
        MENUITEM "Set main reference",          ID_ACTIONS_SPECIAL_SETMAINREFERENCE,MFT_STRING,MFS_ENABLED
        MENUITEM "Add Reference (Colors)\tCtrl+R", ID_ACTIONS_ADDREFERENCE,MFT_STRING,MFS_ENABLED
        MENUITEM "R&euse reference\tCtrl+E",    ID_ACTIONS_REUSEREFERENCE,MFT_STRING,MFS_ENABLED
        MENUITEM "NO reuse center",             ID_ACTIONS_SPECIAL_NOREUSECENTER,MFT_STRING,MFS_ENABLED
        MENUITEM "Show Iterations",             ID_ACTIONSSHOWITERATIONS,MFT_STRING,MFS_ENABLED
