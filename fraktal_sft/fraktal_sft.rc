        MENUITEM "Reset Rotation\tCtrl+Shift+T",ID_ACTIONS_RESETROTATION,MFT_STRING,MFS_ENABLED
        MENUITEM "S&kew\tCtrl+K",               ID_ACTIONS_SKEW,MFT_STRING,MFS_ENABLED
        MENUITEM "Set ratio",                   ID_ACTIONS_SPECIAL_SPECIAL_SETRATIO,MFT_STRING,MFS_ENABLED
        MENUITEM "Reset ratio\tCtrl+Shift+K",   ID_ACTIONS_SPECIAL_SPECIAL_RESETRATIO,MFT_STRING,MFS_ENABLED
        MENUITEM "Skew animation",              ID_SPECIAL_SKEWANIMATION,MFT_STRING,MFS_ENABLED
    END

    POPUP "&Navigation",65535,MFT_STRING,MFS_ENABLED
    BEGIN
        MENUITEM "Newton-Raphson zooming\tCtrl+D", ID_SPECIAL_NEWTON,MFT_STRING,MFS_ENABLED
        MENUITEM "Find &Minibrot\tCtrl+M",      ID_ACTIONS_FINDMINIBROT,MFT_STRING,MFS_ENABLED
        MENUITEM "Non-exact find Minibrot",     ID_SPECIAL_NONEXACTFINDMINIBROT,MFT_STRING,MFS_ENABLED
        MENUITEM "Center c&ursor\tCtrl+U",      ID_ACTIONS_CENTERCURSOR,MFT_STRING,MFS_ENABLED
