            MENUITEM "Fast",                           ID_SPECIAL_PRESET_FAST,MFT_STRING,MFS_ENABLED
            MENUITEM "Best",                           ID_SPECIAL_PRESET_BEST,MFT_STRING,MFS_ENABLED
        END
