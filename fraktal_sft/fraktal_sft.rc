        MENUITEM "Use NanoMB2 (experimental)",  ID_ACTIONS_SPECIAL_USENANOMB2,MFT_STRING,MFS_ENABLED
        MENUITEM "Use auto iterations",         ID_ACTIONS_SPECIAL_AUTOITERATION,MFT_STRING,MFS_ENABLED
