        MENUITEM "Show crosshair window",       ID_SPECIAL_SHOWCROSSHAIR,MFT_STRING,MFS_ENABLED
    END

    POPUP "&Advanced",                       65535,MFT_STRING,MFS_ENABLED
    BEGIN
        MENUITEM "Auto solve glitches",         ID_ACTIONS_AUTOSOLVEGLITCHES,MFT_STRING,MFS_ENABLED
