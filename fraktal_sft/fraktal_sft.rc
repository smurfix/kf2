        MENUITEM "&Exit",                       ID_ACTIONS_EXIT,MFT_STRING,MFS_ENABLED
    END
    
    POPUP "Fra&ktal",65535,MFT_STRING,MFS_ENABLED
    BEGIN
        POPUP "Presets",65535,MFT_STRING,MFS_ENABLED
