<formula type="44" name="TheRedshiftRider 3: 2 * z^2 - z^3 + c" power="3" glitch="0.0001">
  <reference t="C">
    Xn = 2 * (X^2) - (X^3) + C;
  </reference>
  <perturbation t="C">
    xn = 2 * (2 * X * x + x2) - 3 * X2 * x - 3 * X * x2 - x2 * x + c;
  </perturbation>
  <derivative t="C">
    dn = (4 * Xx - 3 * Xx2) * d + d0;
  </derivative>
</formula>
