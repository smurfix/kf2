<formula type="94" name="Sinbrot" power="2" glitch="0.0001">
  <!-- FIXME critical point not 0, not really divergent -->
  <reference t="C">
    Xn = sin(X) + C;
  </reference>
  <perturbation t="C">
    xn = c + 2 * sin(x / 2) * cos(X + (x / 2));
  </perturbation>
  <derivative t="C">
    dn = d0 + d * cos(Xx);
  </derivative>
</formula>
